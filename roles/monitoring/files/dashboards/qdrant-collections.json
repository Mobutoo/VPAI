  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Collection Stats",
      "type": "row"
    },
    {
      "title": "semantic_cache Points",
      "description": "Number of cached embedding vectors in the semantic_cache collection",
      "type": "stat",
      "id": 1,
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "qdrant_points_total{collection=\"semantic_cache\"}",
          "legendFormat": "semantic_cache",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 100 },
              { "color": "yellow", "value": 50000 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "title": "content_index Points",
      "description": "Number of indexed content vectors in the content_index collection",
      "type": "stat",
      "id": 2,
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "qdrant_points_total{collection=\"content_index\"}",
          "legendFormat": "content_index",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null },
              { "color": "green", "value": 100 },
              { "color": "yellow", "value": 50000 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "title": "Qdrant Memory Usage",
      "description": "Current memory consumption of the Qdrant container",
      "type": "gauge",
      "id": 3,
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name=~\".*qdrant.*\"}",
          "legendFormat": "Qdrant memory",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0,
          "max": 2147483648,
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1073741824 },
              { "color": "orange", "value": 1610612736 },
              { "color": "red", "value": 1932735283 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "orientation": "auto"
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "id": 101,
      "title": "Performance",
      "type": "row"
    },
    {
      "title": "Search Latency by Collection",
      "description": "Average search duration for each Qdrant collection",
      "type": "timeseries",
      "id": 4,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "qdrant_search_avg_duration_seconds",
          "legendFormat": "search — {{ collection }}",
          "refId": "A"
        },
        {
          "expr": "qdrant_update_avg_duration_seconds",
          "legendFormat": "update — {{ collection }}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": true,
            "lineWidth": 2,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "thresholdsStyle": { "mode": "line+area" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "title": "REST Operations by Method",
      "description": "Rate of Qdrant REST API operations broken down by HTTP method and status",
      "type": "timeseries",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "sum by (method, status) (rate(qdrant_rest_responses_total[5m]))",
          "legendFormat": "{{ method }} [{{ status }}]",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": true,
            "lineWidth": 2,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "thresholdsStyle": { "mode": "off" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*\\[error\\].*|.*\\[5[0-9][0-9]\\].*" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "id": 102,
      "title": "Resource Usage",
      "type": "row"
    },
    {
      "title": "Qdrant CPU Usage",
      "description": "CPU utilization of the Qdrant container as a percentage",
      "type": "timeseries",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\".*qdrant.*\"}[5m]) * 100",
          "legendFormat": "Qdrant CPU %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": true,
            "lineWidth": 2,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "thresholdsStyle": { "mode": "line+area" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      }
    },
    {
      "title": "Qdrant Network I/O",
      "description": "Network receive and transmit rates for the Qdrant container",
      "type": "timeseries",
      "id": 7,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name=~\".*qdrant.*\"}[5m])",
          "legendFormat": "RX bytes/s",
          "refId": "A"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name=~\".*qdrant.*\"}[5m])",
          "legendFormat": "TX bytes/s",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": true,
            "lineWidth": 2,
            "stacking": { "mode": "none", "group": "A" },
            "axisPlacement": "auto",
            "thresholdsStyle": { "mode": "off" }
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "TX bytes/s" },
            "properties": [
              { "id": "custom.transform", "value": "negative-Y" }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["qdrant", "vector-db"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Qdrant Collections",
  "uid": "qdrant-collections",
  "version": 1
}
