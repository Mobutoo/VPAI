          "rawSql": "WITH daily_stats AS (SELECT model, CASE WHEN EXTRACT(EPOCH FROM (NOW() - first_seen)) > 86400 THEN total_cost / (EXTRACT(EPOCH FROM (NOW() - first_seen)) / 86400) ELSE total_cost END AS daily_cost, CASE WHEN EXTRACT(EPOCH FROM (NOW() - first_seen)) > 86400 THEN total_calls::numeric / (EXTRACT(EPOCH FROM (NOW() - first_seen)) / 86400) ELSE total_calls::numeric END AS daily_calls, CASE WHEN EXTRACT(EPOCH FROM (NOW() - first_seen)) > 86400 THEN total_tokens::numeric / (EXTRACT(EPOCH FROM (NOW() - first_seen)) / 86400) ELSE total_tokens::numeric END AS daily_tokens, first_seen, total_cost, total_calls, total_tokens FROM model_scores WHERE total_calls > 0 ) SELECT model AS \"Model\", ROUND(daily_cost::numeric, 4) AS \"Daily Cost ($)\", ROUND(daily_calls, 1) AS \"Daily Calls\", ROUND(daily_tokens, 0) AS \"Daily Tokens\", ROUND((daily_cost * 7)::numeric, 3) AS \"1 Week ($)\", ROUND((daily_cost * 30)::numeric, 2) AS \"1 Month ($)\", ROUND((daily_cost * 90)::numeric, 2) AS \"3 Months ($)\", ROUND((daily_cost * 365)::numeric, 2) AS \"1 Year ($)\", ROUND((daily_cost * 30)::numeric * 0.92, 2) AS \"EUR/Month\", ROUND(total_cost::numeric, 4) AS \"Spent So Far ($)\", first_seen::date AS \"Tracking Since\" FROM daily_stats ORDER BY daily_cost DESC",
