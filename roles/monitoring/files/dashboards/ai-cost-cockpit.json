      "title": "Model Comparison (PostgreSQL Historical)",
      "type": "row"
    },
    {
      "title": "Model Cost Efficiency Ranking",
      "description": "All models ranked by cost per 1K tokens (lower is better). Compares primary, fallback, and premium tiers",
      "type": "table",
      "id": 14,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 33 },
      "datasource": { "type": "postgres", "uid": "PostgreSQL-n8n" },
      "targets": [
        {
          "rawSql": "SELECT model AS \"Model\", CASE WHEN model IN ('claude-opus', 'claude-sonnet', 'perplexity-pro', 'gpt-codex') THEN 'Premium' WHEN model IN ('kimi-k2', 'minimax-m25', 'deepseek-r1') THEN 'Primary' WHEN model IN ('deepseek-v3', 'glm-5', 'grok-search') THEN 'Fallback' WHEN model IN ('qwen3-coder') THEN 'Free' ELSE 'Other' END AS \"Tier\", total_calls AS \"Total Calls\", total_tokens AS \"Total Tokens\", ROUND(total_cost::numeric, 4) AS \"Total Cost ($)\", CASE WHEN total_tokens > 0 THEN ROUND((total_cost / total_tokens * 1000)::numeric, 5) ELSE 0 END AS \"Cost/1K Tokens ($)\", ROUND(avg_cost_per_call::numeric, 5) AS \"Avg Cost/Call ($)\", ROUND(avg_latency_ms::numeric, 0) AS \"Avg Latency (ms)\", CASE WHEN total_calls > 0 THEN ROUND(successful_calls::numeric / total_calls * 100, 1) ELSE 100 END AS \"Success %\", score AS \"Score\", likes AS \"Likes\", dislikes AS \"Dislikes\" FROM model_scores WHERE total_calls > 0 ORDER BY CASE WHEN total_tokens > 0 THEN total_cost / total_tokens ELSE 999 END ASC",
          "format": "table",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false,
            "filterable": true
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Tier" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-background", "mode": "gradient" } },
              { "id": "mappings", "value": [
                { "type": "value", "options": { "Premium": { "color": "purple", "index": 0, "text": "Premium" }, "Primary": { "color": "blue", "index": 1, "text": "Primary" }, "Fallback": { "color": "orange", "index": 2, "text": "Fallback" }, "Free": { "color": "green", "index": 3, "text": "Free" }, "Other": { "color": "text", "index": 4, "text": "Other" } } }
              ] },
              { "id": "custom.width", "value": 100 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Cost/1K Tokens ($)" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 5 },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.001 }, { "color": "orange", "value": 0.01 }, { "color": "red", "value": 0.05 }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Total Cost ($)" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 4 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Cost/Call ($)" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 5 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Latency (ms)" },
            "properties": [
              { "id": "unit", "value": "ms" },
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 3000 }, { "color": "orange", "value": 8000 }, { "color": "red", "value": 15000 }] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Score" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "gauge", "mode": "gradient" } },
              { "id": "min", "value": 0 },
              { "id": "max", "value": 100 },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "orange", "value": 30 }, { "color": "yellow", "value": 50 }, { "color": "green", "value": 70 }] } },
              { "id": "custom.width", "value": 180 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Success %" },
            "properties": [
