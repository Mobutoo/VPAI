{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Container CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\".+\"}[5m]) * 100",
          "legendFormat": "{{ name }}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] }
    },
    {
      "title": "Container Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name=~\".+\"}",
          "legendFormat": "{{ name }}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    },
    {
      "title": "Container Network RX",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name=~\".+\"}[5m])",
          "legendFormat": "{{ name }}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] }
    },
    {
      "title": "Container Network TX",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(container_network_transmit_bytes_total{name=~\".+\"}[5m])",
          "legendFormat": "{{ name }}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] }
    },
    {
      "title": "Container Restart Count",
      "type": "stat",
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "increase(container_restart_count{name=~\".+\"}[1h])",
          "legendFormat": "{{ name }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["docker", "containers"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
