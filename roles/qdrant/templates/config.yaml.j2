# {{ ansible_managed }}
# Qdrant configuration
# API key is set via QDRANT__SERVICE__API_KEY environment variable

storage:
  storage_path: /qdrant/storage

service:
  host: 0.0.0.0
  http_port: 6333
  grpc_port: 6334
  enable_tls: false

optimizers:
  default_segment_number: 2

hnsw_index:
  m: {{ qdrant_hnsw_m }}
  ef_construct: {{ qdrant_hnsw_ef_construct }}
  full_scan_threshold: 10000
