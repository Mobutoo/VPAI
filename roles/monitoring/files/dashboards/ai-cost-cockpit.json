    {
      "title": "Cost Savings vs Premium-Only",
      "description": "How much you save by using open-source models. Compares actual spend to hypothetical cost if all calls used claude-sonnet pricing",
      "type": "stat",
      "id": 22,
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 56 },
      "datasource": { "type": "postgres", "uid": "PostgreSQL-n8n" },
      "targets": [
        {
          "rawSql": "WITH actual AS (SELECT SUM(total_cost) AS real_cost, SUM(total_tokens) AS total_tok FROM model_scores WHERE total_calls > 0), hypothetical AS (SELECT total_tok * 0.006 AS claude_cost FROM (SELECT SUM(total_tokens) AS total_tok FROM model_scores WHERE total_calls > 0) t) SELECT ROUND(hypothetical.claude_cost::numeric, 2) AS \"premium_only_cost\", ROUND(actual.real_cost::numeric, 2) AS \"actual_cost\", ROUND((hypothetical.claude_cost - actual.real_cost)::numeric, 2) AS \"savings\", CASE WHEN hypothetical.claude_cost > 0 THEN ROUND(((hypothetical.claude_cost - actual.real_cost) / hypothetical.claude_cost * 100)::numeric, 1) ELSE 0 END AS \"savings_pct\" FROM actual, hypothetical",
          "format": "table",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
