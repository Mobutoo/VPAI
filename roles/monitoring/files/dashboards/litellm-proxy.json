{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "LiteLLM Requests/min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(litellm_requests_total[5m]) * 60",
          "legendFormat": "{{ model }} â€” {{ status }}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },
    {
      "title": "LiteLLM Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(litellm_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95 latency"
        },
        {
          "expr": "histogram_quantile(0.50, rate(litellm_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p50 latency"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },
    {
      "title": "Token Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(litellm_tokens_total{type=\"input\"}[5m]) * 60",
          "legendFormat": "Input tokens/min"
        },
        {
          "expr": "rate(litellm_tokens_total{type=\"output\"}[5m]) * 60",
          "legendFormat": "Output tokens/min"
        }
      ]
    },
    {
      "title": "Estimated Cost ($/hour)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "rate(litellm_spend_total[1h]) * 3600",
          "legendFormat": "$/hour"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD", "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] } },
        "overrides": []
      }
    }
  ],
