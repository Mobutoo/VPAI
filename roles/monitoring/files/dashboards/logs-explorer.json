  "panels": [
    {
      "title": "Log Volume by Container",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "Loki" },
      "targets": [
        {
          "expr": "sum by (container) (count_over_time({job=\"docker\"} [1m]))",
          "legendFormat": "{{ container }}"
        }
      ]
    },
    {
      "title": "Error Logs",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 6 },
      "datasource": { "type": "loki", "uid": "Loki" },
      "targets": [
        {
          "expr": "{job=\"docker\"} |~ \"(?i)(error|err|fatal|panic|exception)\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    },
    {
      "title": "All Logs â€” Filterable",
      "type": "logs",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "loki", "uid": "Loki" },
      "targets": [
        {
          "expr": "{job=\"docker\", container=~\"$container\"}",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["logs", "loki"],
  "templating": {
    "list": [
      {
        "name": "container",
        "type": "query",
        "datasource": { "type": "loki", "uid": "Loki" },
        "query": "label_values({job=\"docker\"}, container)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
